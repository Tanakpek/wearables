
<%= simple_form_for(@wearable) do |f| %>
  <%= f.input :title, as: :string %>

  <%= f.input :photos, as: :file, input_html: { multiple: true } %>

  <%= f.input :description, as: :string %>

  <%= f.input :brand, as: :string %>

  <%= f.input :label, as: :string %>

  <%= f.input :price, as: :float %>


  <%= f.submit "Submit"%>

<% end %>
